init_config:
  metric_request_time: fs.nginx.request_time.seconds
  metric_upstream_response_time: fs.nginx.upstream_response_time.seconds
  groups: relay

instances:
  - name: dmp_http
    server_name: relay-dsp.ad-m.asia
    logfile: dmp_http.log
    protocol: http

  - name: subscriber_http
    server_name: relay-dsp.ad-m.asia
    logfile: subscriber_http.log
    protocol: http

  - name: dmp_https
    server_name: relay-dsp.ad-m.asia
    logfile: dmp_https.log
    protocol: https

  - name: subscriber_https
    server_name: relay-dsp.ad-m.asia
    logfile: subscriber_https.log
    protocol: https
